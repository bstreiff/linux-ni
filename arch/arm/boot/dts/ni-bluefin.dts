/dts-v1/;
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include "ni-expio.dtsi"

/ {
	model = "NI cDAQ-9185/9189";
	compatible = "ni,bluefin", "xlnx,zynq-7000";

	leds {
		compatible = "gpio-leds";

		/* LED_STATUSy on GPIO46 */
		status {
			label = "nilrt:status:yellow";
			gpios = <&gpio0 46 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		/* LED_ACTIVEg on GPIO47 */
		active {
			label = "nilrt:active:green";
			gpios = <&gpio0 47 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		/* Reset switch is on GPIO48 */
		reset_sw {
			label = "reset_sw";
			gpios = <&gpio0 48 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			gpio-key,wakeup;
		};
	};

	gpio_restart {
		compatible = "gpio-restart";

		/* ~PS_FORCE_RESET is on GPIO44 */
		gpios = <&gpio0 44 GPIO_ACTIVE_LOW>;
		priority = <200>;
	};
};

&gem0 {
	status = "okay";
};

&i2c0 {
	status = "okay";

	tmp451@4C {
		compatible = "ti,tmp451";
		reg = <0x4C>;
		vcc-supply = <&regulator_vccpint>;
	};
};

&uart1 {
	status = "okay";
};

&usb0 {
	status = "okay";
	dr_mode = "host";
};

&watchdog0 {
	status = "okay";
	reset-on-timeout;
};
